    "file-loader": "^0.11.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.0",
    "style-loader": "^0.17.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": ">=3.1.11"
  },
